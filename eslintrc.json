{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "standard"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-unused-vars": 1,
        "eol-last": 0,
        "vue/multi-word-component-names": [
            "error",
            {
                "ignores": [
                    "default",
                    "about",
                    "privacy"
                ]
            }
        ]
    }
}